<div .row>
    <div .col-md-12>
        $if null projects
            no projects to display
        $else
            <table>
                $forall Entity _ project <- projects
                    <tr>
                        <td>
                            <a href=@{ProjectR (projectHandle project)}>
                                #{projectName project}
                        <td>
                            #{show (projectShareValue project)}/share

