<div .row>
    <div .col-md-12>
        <table>
            <thead>
                <tr>
                    <th>
                        Name
                    <th>
                        IRC Nick
                    <th>
                        Established
                    $forall role <- roles
                        <th>
                            #{role}
            <tbody>
                $if null users
                    <tr>
                        <td>
                            no users to display
                $else
                    $forall (user_id, user) <- users
                        <tr>
                            <td>
                                <a href=@{UserR $ entityKey user}>
                                    #{userPrintName user}
                            <td>
                                $maybe nick <- userIrcNick $ entityVal user
                                    #{nick}
                            <td>
                                $if isJust $ userEstablishedTs $ entityVal user
                                    &#10004;
                            $forall role <- roles
                                <td>
                                    $maybe roles' <- Map.lookup user_id userRoles
                                        $forall (_, project) <- filterRoles role roles'
                                            <a href=@{ProjectR $ projectHandle project}>
                                                #{projectName project} 

